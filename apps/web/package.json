{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "pnpm next-on-pages",
    "db:generate": "npx prisma generate --schema=../../packages/db/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema=../../packages/db/prisma/schema.prisma",
    "db:no-engine": "npx prisma generate --no-engine --schema=../../packages/db/prisma/schema.prisma",
    "db:reset": "npx prisma migrate reset --schema=../../packages/db/prisma/schema.prisma",
    "db:seed": "npx tsx ../../packages/db/prisma/seed.ts",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@aws-sdk/client-s3": "^3.722.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@huggingface/inference": "^2.8.1",
    "@repo/db": "workspace:*",
    "@repo/store": "workspace:*",
    "@repo/types": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/query-sync-storage-persister": "^5.62.12",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-persist-client": "^5.62.12",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.15.0",
    "groq-sdk": "^0.9.1",
    "lucide-react": "^0.468.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.16",
    "react": "^18",
    "react-confetti": "^6.2.2",
    "react-dom": "^18",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@cloudflare/workers-types": "^4.20241205.0",
    "@gradio/client": "^1.8.0",
    "@types/maplibre-gl": "^1.14.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-next-on-pages": "^1.13.7",
    "maplibre-gl": "^5.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.16",
    "typescript": "^5",
    "vercel": "^39.2.2",
    "wrangler": "^3.95.0"
  }
}
